SubDir  FT2_TOP $(FT2_SRC_DIR) psaux ;

{
  local  _sources ;

  if $(FT2_MULTI)
  {
    _sources = afmparse
               psauxmod
               psconv
               psobjs
               t1cmap
               t1decode
               cf2arrst
               cf2blues
               cf2error
               cf2font
               cf2ft
               cf2hints
               cf2intrp
               cf2read
               cf2stack
               ;
  }
  else
  {
    _sources = psaux ;
  }

  Library  $(FT2_LIB) : $(_sources).c ;
}

# end of src/psaux Jamfile
